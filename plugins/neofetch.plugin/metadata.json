{
	"icon": "neofetch",
	"label": "Neofetch",
	"description": "Mostra Informações sobre seu Sistema similar ao Screenfetch ( Uso via Terminal ). ",
	"license": "MIT",
	"category": "Info/Monitor",
	"scripts": {
		"exec": {
			"label": "Instalar",
			"command": "run-as-root -s install.sh"
